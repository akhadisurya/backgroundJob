<div align="center">
  <h3>Send A message to Us</h3>

  <%= form_for(@contact, :html => {:class => 'form-horizontal' }) do |f| %>
  <%= f.label :name %>:
  <%= f.text_field :name %><br><br>

  <%= f.label :message %>:
  <%= f.text_field :message %><br>
    <div>
      </br>
      <%= f.submit "SUBMIT" %>
    </div>
  <% end %>
</div>
